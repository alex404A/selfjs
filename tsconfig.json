{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strictNullChecks": true,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "noImplicitThis": false
  },
  "include": [
    "src/**/*.ts"
  ]
}